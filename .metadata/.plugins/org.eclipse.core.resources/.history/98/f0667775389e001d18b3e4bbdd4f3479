package while1;

import java.util.Scanner;

public class While1 {
	public static void main(String[]args) {
	System.out.println("Quess the number");
	Scanner scan = new Scanner(System.in);
	int chuslo = 0;
	while (chuslo != 5);
	{
	System.out.println("від 0 до 10");	
	chuslo = scan.nextInt();
	}
	}

}
